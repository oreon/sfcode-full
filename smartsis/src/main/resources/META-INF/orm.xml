			select u from Role u where u.name = ?1
			]]> </query> 
    </named-query> 
  </entity>  
  <!--  MonthlyFee  JPA Configurations -->  
  <entity class="com.oreon.smartsis.fees.MonthlyFee" name="MonthlyFee"> 
