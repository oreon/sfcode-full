<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:simple="http://cxf.apache.org/simple"
      xmlns:soap="http://cxf.apache.org/bindings/soap"
      xmlns:jaxws="http://cxf.apache.org/jaxws"
      xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://cxf.apache.org/bindings/soap http://cxf.apache.org/schemas/configuration/soap.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
http://cxf.apache.org/simple http://cxf.apache.org/schemas/simple.xsd">

	
		<jaxws:endpoint id="employeeWebSvc"  implementor="com.pwc.insuranceclaims.websvc.domain.EmployeeWebServiceImpl"
	 	address="/employeeService" />
	
		<jaxws:endpoint id="departmentWebSvc"  implementor="com.pwc.insuranceclaims.websvc.domain.DepartmentWebServiceImpl"
	 	address="/departmentService" />
	
		<jaxws:endpoint id="customerWebSvc"  implementor="com.pwc.insuranceclaims.websvc.quickclaim.CustomerWebServiceImpl"
	 	address="/customerService" />
	
		<jaxws:endpoint id="policyWebSvc"  implementor="com.pwc.insuranceclaims.websvc.quickclaim.PolicyWebServiceImpl"
	 	address="/policyService" />
	
		<jaxws:endpoint id="claimWebSvc"  implementor="com.pwc.insuranceclaims.websvc.quickclaim.ClaimWebServiceImpl"
	 	address="/claimService" />
	
		<jaxws:endpoint id="dependentWebSvc"  implementor="com.pwc.insuranceclaims.websvc.quickclaim.DependentWebServiceImpl"
	 	address="/dependentService" />
	
		<jaxws:endpoint id="claimDocumentWebSvc"  implementor="com.pwc.insuranceclaims.websvc.quickclaim.ClaimDocumentWebServiceImpl"
	 	address="/claimDocumentService" />
	
		<jaxws:endpoint id="userWebSvc"  implementor="com.pwc.insuranceclaims.websvc.users.UserWebServiceImpl"
	 	address="/userService" />
	
		<jaxws:endpoint id="roleWebSvc"  implementor="com.pwc.insuranceclaims.websvc.users.RoleWebServiceImpl"
	 	address="/roleService" />
	
    
</beans>
	