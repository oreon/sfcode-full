		</transition>
		<transition to="publicAuction" name="proceedTopublicAuction">
		</transition>
	
 
	</task-node>

	<task-node name="publicAuctionSale">
		<task name="publicAuctionSaleTask" >
		</task>
		
 	
 
	</task-node>

