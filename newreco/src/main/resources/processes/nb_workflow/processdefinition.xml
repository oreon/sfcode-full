		</transition>
		<transition to="auctionSaleClose" name="proceedToauctionSaleClose">
		</transition>
	
 
	</task-node>

	<decision name="acceptAppraisalOrder" >
	 <handler class="com.nas.recovery.web.action.workflowmgt.nb_workflow.decisions.acceptAppraisalOrderDecisionHandlder" />
		
