<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="bugManagement">
	
	<swimlane name="initiator">
		<assignment class="com.nas.recovery.web.action.workflowmgt.bugManagement.assign.InitiatorAssignment"  ></assignment>
	</swimlane>

	<swimlane name="developer">
		<assignment class="com.nas.recovery.web.action.workflowmgt.bugManagement.assign.DeveloperAssignment"  ></assignment>
	</swimlane>

	<swimlane name="qa">
		<assignment class="com.nas.recovery.web.action.workflowmgt.bugManagement.assign.QaAssignment"  ></assignment>
	</swimlane>

	
	<task-node name="assignDeveloper">
		<task name="assignDeveloperTask"  swimlane="initiator"
			 duedate="0 business days" notify="true">
		</task>
		
		<transition to="reviewIssue" name="assign">
		</transition>
		<transition to="nonissue" name="nonissue">
		</transition>
	
 
	</task-node>`

	<task-node name="reviewIssue">
		<task name="reviewIssueTask"  swimlane="developer"
			 duedate="0 business days" notify="true">
		</task>
		
		<transition to="assignDeveloper" name="reject">
		</transition>
		<transition to="workOnIssue" name="accept">
		</transition>
	
 
	</task-node>`

 	<start-state name="">
		<task name="Task" swimlane="initiator">
		</task>
		<transition to="reviewIssue" name="proceedToreviewIssue"></transition>
		
	</start-state>
 
	<task-node name="verifyfix">
		<task name="verifyfixTask"  swimlane="qa"
			 duedate="0 business days" notify="true">
		</task>
		
		<transition to="notifyClose" name="close">
		</transition>
		<transition to="workOnIssue" name="requestModification">
		</transition>
	
 
	</task-node>`

 	<end-state name="fixed"></end-state>
 
	<task-node name="workOnIssue">
		<task name="workOnIssueTask"  swimlane="developer"
			 duedate="2 business days" notify="false">
		</task>
		
		<transition to="verifyfix" name="fixed">
		</transition>
	
 
	</task-node>`

 	<end-state name="nonissue"></end-state>
 
	<task-node name="notifyClose">
		<task name="notifyCloseTask" swimlane="qa" notify="true">
		</task>
		
		<transition to="fixed" name="close">
		</transition>
	
 
	</task-node>

</process-definition>
	