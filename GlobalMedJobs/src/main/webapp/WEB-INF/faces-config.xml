<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="1.2"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <!-- Facelet support -->
    <application>
		<view-handler>com.icesoft.faces.facelets.D2DSeamFaceletViewHandler</view-handler>
   	</application>
   	
   	<!-- Tree example bean-->
    <managed-bean>
        <description>Backing bean for tree.</description>
        <managed-bean-name>treeController</managed-bean-name>
        <managed-bean-class>com.fis.mlportal.view.component.TreeController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
</faces-config>
