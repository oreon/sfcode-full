
	<html 
 xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	xmlns:c="http://java.sun.com/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions" 
	xmlns:wc="http://witchcraft.sourceforge.net/jsf"
 >	
	
		<ice:commandButton value="Create New" action="edit" rendered="#{identity.loggedIn}"/>
		<ice:dataTable  var="encounter" value="#{encounterList}" rows="30"
		rowClasses="odd, even" styleClass="dataTable" >		
		
	
	<ice:column>
		<f:facet name="header">
			<h:outputText value="#{msg.encounter_notes}" />
		</f:facet>
		<h:outputText escape="false"  value="#{encounter.notes}" />
	</ice:column>
	
		
			
				
	<ice:column >
		<f:facet name="header">
			<h:outputText value="#{msg.encounter_patient}"   /> 	
		</f:facet>
		<s:link   value="#{encounter.patient.displayName}" action="viewPatient" />
	</ice:column>
			
		
	
		
		
	<ice:column>
		<f:facet name="header">
			<h:outputText value="#{msg.dateCreated}" />
		</f:facet>
		<h:outputText value="#{encounter.dateCreated}" />
	</ice:column>

	
		
	<ice:column   >
		<f:facet name="header">
			<h:outputText value=" " />
		</f:facet>
		<h:commandLink 
			action="#{encounterAction.select(encounter)}">
			<h:graphicImage value="/images/icons/eye.png" style="border: 0px" />
		</h:commandLink>
	</ice:column>

		
	<ice:column   >
		<f:facet name="header">
			<h:outputText value=" " />
		</f:facet>
		<h:commandLink 
			action="#{encounterAction.select(encounter)}">
			<h:graphicImage value="/images/icons/edit.png" style="border: 0px" />
		</h:commandLink>
	</ice:column>

		
	<ice:column   >
		<f:facet name="header">
			<h:outputText value=" " />
		</f:facet>
		<h:commandLink 
			action="#{encounterAction.showDeleteDialog(encounter)}">
			<h:graphicImage value="/images/icons/delete.png" style="border: 0px" />
		</h:commandLink>
	</ice:column>

		</ice:dataTable>
		
		<wc:deletePanel backingAction="#{encounterAction}" backingEntity="#{encounter}" title="Delete Encounter" />
	</html>
	