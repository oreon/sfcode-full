
	<html 
 xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	xmlns:c="http://java.sun.com/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions" 
	xmlns:wc="http://witchcraft.sourceforge.net/jsf"
 >	
	<ice:panelGroup  >
		<s:validateAll>
		<h:panelGrid columns="3" styleClass="viewInfo" columnClasses="leftCol, rightCol">
		<f:facet name="header">
			<h:outputText value="Complaint " />
		</f:facet>
		
	
	
		
			
	
	
		
		<h:outputText value="#{msg.complaint_encounter}  "/>
		
	
	<h:selectOneMenu id="Complaint_encounter" value="#{complaint.encounter}" required="true">
		<s:selectItems value="#{encounterList}" var="itemIter"
			label="#{itemIter.displayName}"
			noSelectionLabel="Please Select..." />
		<s:convertEntity />
	</h:selectOneMenu>
	
	

		
		<h:message styleClass="error errors" for="Complaint_encounter" />
		
		
	

	
		
	


 
	
		
			
	
	
		
		<h:outputText value="#{msg.complaint_symptom}  "/>
		
	
	<h:selectOneMenu id="Complaint_symptom" value="#{complaint.symptom}" required="true">
		<s:selectItems value="#{symptomList}" var="itemIter"
			label="#{itemIter.displayName}"
			noSelectionLabel="Please Select..." />
		<s:convertEntity />
	</h:selectOneMenu>
	
	

		
		<h:message styleClass="error errors" for="Complaint_symptom" />
		
		
	

	
		
	


 

	
	
	
	
		
		<h:outputText value="#{msg.complaint_type}  "/>
		
	
	enm ce:contactDetails 
	<h:selectOneMenu id="Complaint_type" value="#{complaint.type}" required="false">
		<f:selectItem itemLabel="Please Select..." itemValue="" />
		<f:selectItem itemLabel="ACUTE" itemValue="ACUTE" />
		<f:selectItem itemLabel="CHRONIC" itemValue="CHRONIC" />
		<f:selectItem itemLabel="TRANSIENT" itemValue="TRANSIENT" />
		
	</h:selectOneMenu>
	

		
		<h:message styleClass="error errors" for="Complaint_type" />
		
		
	

	
		</h:panelGrid>
		
	
		</s:validateAll>
		
	<div class="buttonBox">
		
		<h:commandButton value="Save" action="#{complaintAction.save}" /> 
		<h:outputText value=" " />
		<h:commandButton value="Cancel" immediate="true" action="#{complaintAction.cancel}" /> 
		
	</div>

	</ice:panelGroup>
	</html>
	