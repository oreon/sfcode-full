
	<html 
 xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ice="http://www.icesoft.com/icefaces/component"
	xmlns:c="http://java.sun.com/jstl/core"
	xmlns:fn="http://java.sun.com/jsp/jstl/functions" 
	xmlns:wc="http://witchcraft.sourceforge.net/jsf"
 >	
	<ice:panelGroup  >
		<s:validateAll>
		<h:panelGrid columns="3" styleClass="viewInfo" columnClasses="leftCol, rightCol">
		<f:facet name="header">
			<h:outputText value="Floor " />
		</f:facet>
		
	

	
	
	
	
		
		<h:outputText value="#{msg.floor_number}  "/>
		

	<ice:inputText  id="Floor_number" required="false"  value="#{floor.number}" />
   

		
		<h:message styleClass="error errors" for="Floor_number" />
		
		
	

	
		</h:panelGrid>
		
	
	
	
	
	<h:commandButton  value="New Ward" action="#{floorAction.addWards}" >
		<s:conversationId/>
	</h:commandButton>

	
	
	<ice:dataTable id="wards_data" var="ward" rendered="#{not empty floorAction.listWards}"
					value="#{floorAction.listWards}" rows="10"
				rowClasses="odd, even" styleClass="dataTable" rules="all">
		
	
	
	

	
	
	
	
		
		<h:column style="text-align:right"><f:facet name="header"><h:outputText value="#{msg.ward_name} * "/></f:facet>
		

	<ice:inputText  id="Ward_name" required="true"  value="#{ward.name}" />
   

		
		<h:message styleClass="error errors" for="Ward_name" />
		</h:column>
		
	

	

	
	
		
		<h:column style="text-align:right"><f:facet name="header"><h:outputText value="#{msg.ward_allowedGender}  "/></f:facet>
		
	
	<h:selectOneMenu id="Ward_allowedGender" value="#{ward.allowedGender}" required="false">
		<f:selectItem itemLabel="Please Select..." itemValue="" />
		<f:selectItem itemLabel="F" itemValue="F" />
		<f:selectItem itemLabel="M" itemValue="M" />
		
	</h:selectOneMenu>
	

		
		<h:message styleClass="error errors" for="Ward_allowedGender" />
		</h:column>
		
	

	

	
	
		
		<h:column style="text-align:right"><f:facet name="header"><h:outputText value="#{msg.ward_price}  "/></f:facet>
		

	<ice:inputText  id="Ward_price" required="false"  value="#{ward.price}" />
   

		
		<h:message styleClass="error errors" for="Ward_price" />
		</h:column>
		
	

	
		
		
		
	<h:column>
		
	<h:commandButton  value="New Ward" action="#{floorAction.addWards}" >
		<s:conversationId/>
	</h:commandButton>

	</h:column>

		
	<h:column>
		<ice:commandButton value="Delete" action="#{floorAction.deleteWards(ward)}" immediate="true">
			<s:conversationId/>
		</ice:commandButton>
	</h:column>

		
			
	</ice:dataTable>
		</s:validateAll>
		
	<div class="buttonBox">
		
		<h:commandButton value="Save" action="#{floorAction.save}" /> 
		<h:outputText value=" " />
		<h:commandButton value="Cancel" immediate="true" action="#{floorAction.cancel}" /> 
		
	</div>

	</ice:panelGroup>
	</html>
	